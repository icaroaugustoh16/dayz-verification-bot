{
  "name": "dayz-discord-automation",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Sistema completo de automação para servidor DayZ",
  "main": "bot-new.js",
  "scripts": {
    "start": "node bot-new.js",
    "bot-old": "node bot.js",
    "dev": "nodemon bot-new.js",
    "deploy": "node deploy-commands.js",
    "clear-commands": "node clear-commands.js",
    "api": "node server.js",
    "verify": "node verification-server.js",
    "monitor": "node be-log-monitor.js",
    "leaderboard": "node leaderboard-sync.js",
    "maintenance": "node maintenance.js",
    "migrate": "node migrate-players.js",
    "all": "concurrently \"npm run api\" \"npm run verify\" \"npm run start\" \"npm run monitor\" \"npm run leaderboard\"",
    "setup": "node setup.js"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.0",
    "mercadopago": "^2.9.0",
    "mongodb": "^6.3.0",
    "ngrok": "^5.0.0-beta.2",
    "passport": "^0.7.0",
    "passport-steam": "^1.0.18"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
